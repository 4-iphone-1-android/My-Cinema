<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Screen.HomeActivity">
    <com.shrikanthravi.customnavigationdrawer2.widget.SNavigationDrawer
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:navigationDrawerBackgroundColor="#FFC9C9"
        app:appbarTitleTextColor="#041C45"
        app:HamMenuIconTintColor="#041C45"
        app:appbarColor="#FFC9C9"
        app:primaryMenuItemTextColor="#041C45"
        app:secondaryMenuItemTextColor="#041C45"
        app:appbarTitleTextSize="7sp"
        app:primaryMenuItemTextSize="7sp"
        app:secondaryMenuItemTextSize="7sp"
        android:id="@+id/navigationDrawer">

        <FrameLayout
            android:id="@+id/frameLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </com.shrikanthravi.customnavigationdrawer2.widget.SNavigationDrawer>
</androidx.constraintlayout.widget.ConstraintLayout>